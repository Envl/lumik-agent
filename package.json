{
    "name": "llm-browser",
    "version": "1.0.0",
    "description": "Lumik Agent - A general-purpose AI agent that can perform any browser task through natural language commands",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "vite",
        "dev:hotreload": "node scripts/dev-hotreload.js",
        "dev:reload": "node scripts/reload-extension.js",
        "build": "vite build && node post-build.js",
        "build:watch": "vite build --watch",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "browser-extension",
        "ai",
        "automation",
        "svelte"
    ],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.6.3",
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^6.1.0",
        "@tailwindcss/vite": "^4.1.11",
        "@tsconfig/svelte": "^5.0.4",
        "@types/chrome": "^0.1.1",
        "daisyui": "^5.0.46",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "svelte": "^5.36.6",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3",
        "vite": "^7.0.5",
        "vite-plugin-static-copy": "^3.1.1"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@tailwindcss/oxide",
            "esbuild"
        ]
    }
}
